{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
    	"node_modules/grunt-jsdoc/node_modules/jsdoc/plugins/escapeHtml", 
    	"node_modules/grunt-jsdoc/node_modules/jsdoc/plugins/overloadHelper", 
    	"lib/jsdoc/private_methods.js",
    	"lib/jsdoc/tabs.js",
    	"node_modules/grunt-jsdoc/node_modules/jsdoc/plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}